<div class="jumbotron">
    <h1>Amar2000 backend</h1>
    <p class="lead">Congratulations, you have found our backend endpoint. Please leave.</p>
    <p>
        @using (Html.BeginForm("ForceUpdate", "Home", FormMethod.Post, new { @id = "force-update-form" }))
        {
            @Html.AntiForgeryToken()
            <a href="#" onclick="document.getElementById('force-update-form').submit();" class="btn btn-primary btn-lg">
                Force update to connected clients &raquo;
            </a>
        }

        @using (Html.BeginForm("FakeIt", "Home", FormMethod.Post, new { @id = "force-update-form2" }))
        {
            @Html.AntiForgeryToken()
            <a href="#" onclick="document.getElementById('force-update-form2').submit();" class="btn btn-primary btn-lg">
                Force FAKE update to connected clients &raquo;
            </a>
        }
    </p>
    <p></p>
</div>
